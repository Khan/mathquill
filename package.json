{
  "name": "mathquill",
  "description": "Easily type math in your webapp",
  "version": "0.10.1",
  "license": "MPL-2.0",
  "main": "dist/src/publicapi.js",
  "types": "dist/src/publicapi.d.ts",
  "repository": {
